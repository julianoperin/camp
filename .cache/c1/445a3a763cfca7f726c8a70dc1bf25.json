":{"line":289,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":37,"column":0},"generated":{"line":292,"column":0}},{"source":"sass/main.scss","name":null,"original":{"line":38,"column":2},"generated":{"line":293,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":39,"column":2},"generated":{"line":294,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":43,"column":0},"generated":{"line":297,"column":0}},{"source":"sass/main.scss","name":null,"original":{"line":44,"column":2},"generated":{"line":298,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":45,"column":2},"generated":{"line":299,"column":2}}],"sources":{"sass/_app.scss":"$background-color: #fff;\n$text-color: #000;\n\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\nbody {\n  background: $background-color;\n  font-family: \"Forum\", cursive;\n  color: $text-color;\n  overflow-x: hidden;\n  position: relative;\n  background: #fafafa;\n  // cursor: none;\n}\n\nmain {\n  position: absolute;\n  z-index: -1;\n}\n\n// SLIDE AND DESCRIPTION\n.slide {\n  min-height: 90vh;\n  margin: 0 auto 0 auto;\n  width: 84%;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  color: $text-color;\n  background: #fafafa;\n\n  .hero-desc {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    flex: 1;\n    flex-basis: 25rem;\n    padding-left: 3.5rem;\n    position: relative;\n  }\n\n  .hero-img {\n    position: relative;\n    flex: 1;\n    flex-basis: 35rem;\n    overflow: hidden;\n  }\n\n  .hero-img img {\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n  }\n\n  .fashion-span {\n    color: rgb(86, 124, 228);\n  }\n\n  .mountain-span {\n    color: rgb(86, 124, 228);\n  }\n\n  .hike-span {\n    color: rgb(86, 124, 228);\n  }\n\n  .hero-desc h2 {\n    font-family: \"Forum\", cursive;\n    font-size: 3rem;\n  }\n\n  .hero-desc p {\n    padding: 2rem 0rem;\n    font-size: 1.5rem;\n  }\n  .explore {\n    padding: 1rem 2rem;\n    background: none;\n    color: $text-color;\n    border: 1px solid $text-color;\n    font-size: 1rem;\n    text-decoration: none;\n    align-self: flex-start;\n    cursor: pointer;\n    transition: all 2s ease;\n  }\n}\n\n.reveal-img {\n  position: absolute;\n  width: 100%;\n  height: 150%;\n  background: #fafafa;\n  left: 0;\n  top: -15%;\n}\n","sass/_nav.scss":"header {\n}\n\nnav {\n  display: flex;\n  align-items: center;\n  height: 8vh;\n  // background: #fafafa;\n  position: relative;\n  z-index: 5;\n}\n\n.logo {\n  color: black;\n  text-transform: uppercase;\n  font-size: 26px;\n  letter-spacing: 5px;\n  flex: 1;\n  margin-left: 20px;\n}\n\n.nav-links {\n  display: flex;\n  justify-content: space-around;\n  align-items: items;\n  width: 40%;\n  flex: 1;\n  position: relative;\n}\n\n.nav-links li {\n  padding: 1rem;\n  list-style: none;\n  z-index: 5;\n}\n\n.nav-links li a {\n  color: black;\n  text-decoration: none;\n  letter-spacing: 3px;\n  font-weight: bold;\n  font-size: 14px;\n}\n\n.burger {\n  display: none;\n  margin-right: 2rem;\n  cursor: pointer;\n}\n\n.burger div {\n  width: 30px;\n  height: 3px;\n  background-color: black;\n  margin: 5px;\n  transition: all 0.3s ease;\n}\n\n// ! MEDIA QUERY 768px\n@media screen and (max-width: 768px) {\n  .nav-links {\n    position: absolute;\n    right: 0px;\n    height: 92vh;\n    top: 8vh;\n    background-color: #fafafa;\n    color: black;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    width: 100%;\n    clip-path: circle(10px at 90% -20%);\n    -webkit-clip-path: circle(100px at 90% -20%);\n    transition: all 0.5s ease-in;\n    pointer-events: none;\n  }\n\n  .burger {\n    display: block;\n  }\n\n  .nav-active {\n    clip-path: circle(100px at 90% -10%);\n    -webkit-clip-path: circle(1200px at 90% -10%);\n    pointer-events: all;\n  }\n\n  /* nav links animation */\n  .nav-links li {\n    opacity: 0;\n  }\n}\n\n//! Links Animation\n@keyframes navLinkFade {\n  from {\n    opacity: 0;\n    transform: translateX(150px);\n  }\n  to {\n    opacity: 1;\n    transform: translateX(0px);\n  }\n}\n\n//! Toggle burger animation\n.toggle .line1 {\n  transform: rotate(-45deg) translate(-5px, 6px);\n}\n.toggle .line2 {\n  opacity: 0;\n}\n.toggle .line3 {\n  transform: rotate(45deg) translate(-5px, -6px);\n}\n\n//! No scroll when mobile view\n.over {\n  overflow: hidden;\n}\n\n.hovered-link {\n  color: white;\n}\n","sass/_cursor.scss":"//  CURSOR\n.cursor {\n  width: 2.5rem;\n  height: 2.5rem;\n  border: 2px black solid;\n  border-radius: 50%;\n  position: absolute;\n  transform: translate(-50%, -50%);\n  pointer-events: none;\n  // animation for the cursor\n  transition: all 0.5s ease-in-out;\n  transition-property: background, transform;\n  transform-origin: 75% 75%;\n}\n\n.cursor.logo-active {\n  background: rgb(86, 124, 228);\n  transform: scale(2.5);\n  z-index: 1;\n  border: 2px solid black;\n}\n\n// burger\n.cursor.burger-active {\n  background: rgb(86, 124, 228);\n  transform: scale(1.5);\n  border: 2px solid black;\n  z-index: 1;\n  transform-origin: 120% 120%;\n}\n\n.cursor.explore-active {\n  background: black;\n  color: white;\n  transform: scale(3);\n}\n.cursor-text {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  font-size: 0.5rem;\n  transform: translate(-50%, -50%);\n}\n\n// IMG\n.blur {\n  border: 2px solid white;\n  transform: scale(1.2);\n  transform-origin: 100px 100px;\n}\n\n.cursor.link-grow {\n  transform: scale(2);\n  background: rgb(86, 124, 228);\n  transform-origin: 100% 100%;\n}\n","sass/main.scss":"@import \"app\";\n@import \"nav\";\n@import \"cursor\";\n// @import \"work\";\n\n/* \nfont-family: 'Forum', cursive;\nfont-family: 'Give You Glory', cursive;\nfont-family: 'Quicksand', sans-serif;\n*/\n\n// ! Helper functions\n.flex {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n}\n\n// ! Page Transitions\n.swipe {\n  position: fixed;\n  width: 100%;\n  height: 100%;\n  top: 0;\n  left: 0;\n  transform: translateX(-100%);\n}\n.swipe1 {\n  background: black;\n  z-index: 7;\n}\n.swipe2 {\n  background: white;\n  z-index: 6;\n}\n.swipe3 {\n  background: black;\n  z-index: 5;\n}\n\n// type writer\n.name {\n  letter-spacing: 0.5rem;\n  color: rgb(86, 124, 228);\n}\n"},"lineCount":null}},"error":null,"hash":"9a767e649de6a90b26afd9594f426e8a","cacheData":{"env":{}}}